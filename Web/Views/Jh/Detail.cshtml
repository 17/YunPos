    };

    //审核
    app.jh.detail.sh = function (id, obj) {
        $(obj).trigger('blur');
        $.messager.confirm("提示", "确定审核吗?", function () {
            $.post($.DHB.U('jh/sh'),
            {
                id: id
            },
                function (data) {
                    if (data.status == "success") {
                        $.DHB.message({ 'content': data.message, 'time': 1000, 'type': 's' });
                        $.DHB.url('jh/detail?id=' + id, 'cache');
                    }
                    else {
                        $.DHB.message({ 'content': data.message, 'time': 0, 'type': 'e' });
                    }
                }, 'json');
        });
    };


    //退货
    app.jh.detail.th = function (id, obj) {
