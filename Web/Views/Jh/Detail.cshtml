        $('#shopsp_table_jh_detail>tbody', _).find('tr').each(function () {
            $(this).remove();
        });
    }

    app.jh.detail.clear_select_dh = function (el) {
        var select_callback = function () {
            var selectCallback = $(el).data('callback');
            if (selectCallback) {
                eval(selectCallback + '();');
            }
        }

        var id_bill_origin = $(_ + "#id_bill_origin").val();
        if (id_bill_origin != '') {
            app.jh.detail.removeAll();
            app.jh.detail.setresult(true);
        }

        $(_ + '#' + $(el).data('id')).val('');
        $(_ + '#' + $(el).data('name')).val('');
        select_callback();
    };





    //编辑收货
    app.jh.detail.edit = function (id, obj) {
        $.fn.menuTab.load({ url: '/jh/add?type=edit&id=' + id, 'title': '收货管理', id: 'jh/add', nocache: '0' });
    };

