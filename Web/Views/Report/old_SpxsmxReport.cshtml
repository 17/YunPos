<script type="text/javascript">
    app.report = app.report || {};
    app.report.get_select_xsmx = function () {
        debugger;
        var id_shop = "";
        $.DHB.dialog({ 'title': '选择商品', 'url': $.DHB.U('shopsp/search?searchType=report&count=1&id_shop=' + id_shop), 'id': 'dialog-shopsp-search', 'confirm': app.report.dialogCallBack_xsmx });
    }
    app.report.dialogCallBack_xsmx = function (array) {
        var jsonStr = "";
        $.each(array, function (index, item) {
            if (item.name == "shopsp_table_json") {
                jsonStr = item.value;
            }
        });
        var objList = jQuery.parseJSON(jsonStr);
        debugger;
        if (objList.length > 0) {
            $(_ + "#s_id_shopsp_mc").val(objList[0].mc);
            $(_ + "#s_id_shopsp").val(objList[0].id_shopsp);
        }
        $.DHB.dialog({ 'id': 'dialog-shopsp-search', 'action': 'destroy' });
        app.search.do_search_report_spxsmxreport();
    }
    //分页回调
    app.report.spxsmxreport_pagination_callback = function (data, textStatus, idx, jq) {
        app.report.spxsmxreport_search_callback(data, textStatus);
    };

    //查询回调
    app.report.spxsmxreport_search_callback = function (data, textStatus) {
        var sort_array = $('.table a[data-sort]', _);
        var $current_sort = ($('#hid_sort', _).val().toLowerCase()).split(' ', 2);       //这是当前排序

        //绑定UI
        sort_array.each(function (index, obj) {
            var $current = $(obj);
            if ($current.attr('data-sort') === $current_sort[0]) {
                $current.attr('data-dir', $current_sort[1]);
                if ($current_sort[1] === 'asc')
                    $current.append(app.sort_icon.asc);      //升序
                else
                    $current.append(app.sort_icon.desc);     //降序
            }
            else {
                $current.append(app.sort_icon.sort);
            }
        });

        //绑定事件
        sort_array.off('click').on('click', function (e) {
            var e = e || event;
            var $current = $(e.target);        //获取当前点击派序列
            var _dir = 'desc';
            if ($.trim($current.attr('data-dir')).length == 0 || $.trim($current.attr('data-dir')) === 'asc') {
                $current.attr('data-dir', _dir);
            }
            else {
                _dir = 'asc';
                $current.attr('data-dir', _dir);
            }
            $('#hid_sort', _).val($current.attr('data-sort') + ' ' + _dir);
            app.search.do_search_report_spxsmxreport();
        });
    };

    //编辑单位
    app.report.edit = function (id, obj) {
        $(obj).trigger('blur');
        $.DHB.dialog({
            'title': '编辑单位',
            'url': $.DHB.U('report/edit?id=' + id),
            'id': 'dialog-report',
            //'confirm': function (array) {
            //}
        });
    };

    //新增单位
    app.report.add = function (obj) {
        $(obj).trigger('blur');
        $.DHB.dialog({
            'title': '新增单位', 'url': $.DHB.U('report/add'), 'id': 'dialog-report',
            //'confirm': function (array) {
            //    alert(array);
            //    debugger;

            //}
        });
    };

    //删除单位
    app.report.del = function (id, obj) {
        $(obj).trigger('blur');
        $.messager.confirm("提示", "确定删除吗?", function () {
            $.post($.DHB.U('report/delete'),
            {
                id: id
            },
                function (data) {
                    if (data.status == "success") {
                        $.DHB.message({ 'content': data.message, 'time': 1000, 'type': 's' });
                        $.DHB.url('report/pscrkmxreport', 'cache');
                    }
                    else {
                        $.DHB.message({ 'content': data.message, 'time': 0, 'type': 'e' });
                    }
                }, 'json');
        });
    };

    //内页加载完成
    app.report.spxsmxreport_ready = function () {
        //Init Sort
        app.report.spxsmxreport_search_callback(null, null);
        $('.close_tree', _).click(function () {
            $(this).parent().toggleClass('close_t');
        });
        $.DHB.loadJs([{ id: 'jstree', url: '/static/js/jstree/jstree.js' }], function () {
            app.report.bind_spfl_tree();
        });
        //绑定左侧商品分类树#modify_goods_info
        app.report.bind_spfl_tree = function () {
            var tree_selector = '#tree_left';

            $(_ + tree_selector)
                .jstree("destroy")
                .jstree({
                    'core': {
                        'data': {
                            'url': function (obj) {
                                var type = $.trim($('#hid_type').val());
                                return '/shopsp/get_left_tree?rd=' + new Date().getTime() + '&type=' + type;
                            }
                        },
                    }
                })
                .on('ready.jstree', function (e, obj) {
                    obj.instance.select_node({ "id": "0" }, true, false);
                    obj.instance.open_node({ "id": "0" });
                })
                .on("changed.jstree", function (e, data) {

                    if (data.selected && data.selected.length) {
                        var i, j, r = [];
                        for (i = 0, j = data.selected.length; i < j; i++) {
                            r.push(data.instance.get_node(data.selected[i]).id);
                        }
                        //TODO：赋值到hidden，触发查询事件等
                        $(_ + "#s_id_spfl").val(r.join(''));
                        // debugger;
                        app.search.do_search_report_spxsmxreport();

                    }
                });


        }

    };
</script>  