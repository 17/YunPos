                                            app.search = app.search || {};

                                            app.search.search_report_spxsmxreport = function () {
                                                app.c.public_data['report/spxsmxreport'] = app.c.public_data['report/spxsmxreport'] || {};
                                                app.c.public_data['report/spxsmxreport']['_row_total_'] = '@_result.outstr';
                                                app.c.public_data['report/spxsmxreport']['_page_size_'] = '@pageSize';
                                                app.c.public_data['report/spxsmxreport']['_current_page_'] = '@(page)';
                                                app.search.do_search_pagination_report_spxsmxreport();
                                            }


                                            app.search.trigger_search_report_spxsmxreport = function (event) {
                                                //绑定回车键
                                                event = event || window.event
                                                if (event.keyCode == 13) {
                                                    app.search.do_search_report_spxsmxreport();
                                                    event.preventDefault();
                                                }
                                            }

                                            //查询
                                            app.search.do_search_report_spxsmxreport = function (el, isExport) {
                                                isExport = isExport || 0;
                                                var v = $(_ + ".filter-form").serialize();
                                                var url = '/report/spxsmxreport?_search_=1&' + v;
                                                if (isExport == 1) {
                                                    url += '&export=1';
                                                    url = $.DHB.U(url);
                                                    window.location.href = url;
                                                }
                                                else {
                                                    var options = {
                                                        url: $.DHB.U(url),
                                                        datatype: 'html',
                                                        beforeSend: function () {
                                                            $.DHB.showButterbar();
                                                        },
                                                        callback: function (data, textStatus, jqXHR) {
                                                            //debugger;
                                                            if ($('#report-spxsmxreport-list-fresh-box', _).length > 0) {
                                                                $('#report-spxsmxreport-list-fresh-box', _).html(data);
                                                            }
                                                            var curSearchcallback = 'app.report.spxsmxreport_search_callback';
                                                            var curListcallback = 'app.report.spxsmxreport_listready';
                                                            eval('try {if(' + curSearchcallback + ' && typeof(' + curSearchcallback + ')=="function"){' + curSearchcallback + '(data, textStatus); }}catch(e){}');
                                                            eval('try {if(' + curListcallback + ' && typeof(' + curListcallback + ')=="function"){' + curListcallback + '(); }}catch(e){}');
                                                            app.search.do_search_pagination_report_spxsmxreport();
                                                        },
                                                        complete: function (XHR, TS) {
                                                            $.DHB.closeButterbar();
                                                        }
                                                    };
                                                    cy.http.Get(options);
                                                }
                                                return false;
                                            };

                                            //分页
                                            app.search.do_search_pagination_report_spxsmxreport = function () {
                                                $.DHB.func.pagination({
                                                    module_name: 'Manager',
                                                    controller_name: 'report',
                                                    action_name: 'spxsmxreport',
                                                    ready_once: false
                                                });
                                            }


                                            app.search.do_search_option_report_spxsmxreport = function (el) {
                                                if ($(el).attr('class') != '') {
                                                    $(el)
                                                        .find('span')
                                                        .each(function () {
                                                            if ($(this).attr('class') == '') {
                                                                $(this).attr('class', 'order-active');
                                                            } else {
                                                                $(this).attr('class', '');
                                                            }
                                                        });

                                                    if ($(el).find('span:eq(0)').attr('class') == '') {
                                                        $(el).parents('.btn-group').find('input:eq(1)').val('desc');
                                                    } else {
                                                        $(el).parents('.btn-group').find('input:eq(1)').val('asc');
                                                    }
                                                }

                                                if ($(el).attr('class') == '') {
                                                    $(el).parent().find('.active').removeClass('active');
                                                    $(el).addClass('active');
                                                    $(el)
                                                        .parents('.btn-group')
                                                        .find('input:eq(0)')
                                                        .val($(el).attr('value'));
                                                }

                                                $(el).parents('.btn-group').removeClass('open');
                                                var selectA = $(el).find('a').clone();
                                                selectA.find('span[class=""]').remove();
                                                $(el)
                                                    .parents('.btn-group')
                                                    .find('button span:eq(0)')
                                                    .html(selectA.html());

                                                app.search.do_search_report_spxsmxreport();
                                            }


                                            app.search.search_enter = function (el) {
                                                if (app.c.public_data['report/spxsmxreport']['search_all'] === false) {
                                                    $(el)
                                                        .parents('.search-box-container')
                                                        .find(".bootstrap-select ul.dropdown-menu")
                                                        .each(function () {
                                                            var ts = $(this).find('li:first() span:first()');
                                                            ts.text('全部');
                                                        });
                                                    /*$(el).parents('.search-box-container').find(".treeview .spxsmxreport-group>li:first()").each(function(){
                                                        $(this).text('全部');
                                                    });*/
                                                    app.c.public_data['report/spxsmxreport']['search_all'] = true;
                                                }
                                            }


                                            app.search.search_leave = function (el) {
                                            }

                                            app.search.search_client_clear_report_spxsmxreport = function (el) {
                                                $(el).parents('.input-group').find('input').val('');
                                                app.search.do_search_report_spxsmxreport();
                                            }


                                            app.search.wdatepicker_report_spxsmxreport = function (el) {
                                                var booStart = $(el).data('type') == 'start';
                                                var option = {};
                                                option['el'] = $(el).data('field') + (!booStart ? '_end' : '');
                                                option['onpicked'] = function () {
                                                    $(el)
                                                        .text($dp.cal.getP('y') +
                                                            '-' +
                                                            $dp.cal.getP('M') +
                                                            '-' +
                                                            $dp.cal.getP('d'));
                                                    if (booStart) {
                                                        setTimeout(function () {
                                                            $(el)
                                                                .parent()
                                                                .find('[data-type="end"]')
                                                                .data('position', '1')
                                                                .click();
                                                        },
                                                            5000);
                                                    }
                                                    app.search.do_search_report_spxsmxreport();
                                                };

                                                if (booStart) {
                                                    option['maxDate'] =
                                                        '#F{ $dp.$D(\'' + $(el).data('field') + '_end\') }';
                                                } else {
                                                    option['minDate'] = '#F{ $dp.$D(\'' + $(el).data('field') + '\') }';
                                                    if ($(el).data('position') == '1') {
                                                        //option['position'] ={left:$(el).offset().left,top:$(el).offset().top+34};
                                                        //$(el).data('position','');
                                                    }
                                                }
                                                option['oncleared'] = function () {
                                                    $(el).html(booStart ? $(el).data('title') : '截至日期');
                                                    app.search.do_search_report_spxsmxreport();
                                                };

                                                WdatePicker(option);
                                            }


                                            app.search.search_item_clear_report_spxsmxreport = function (el) {
                                                var par = $(el).parents('form');
                                                par.get(0).reset();
                                                par.find("select.select2_")
