                                                                }
                                                                @*@if (IsPermissionShow("zf", actionlist)&&item.flag_sh==1&&item.flag_cancel==0&&item.flag_delete==0)
                                                                {
                                                                    <a class="bg-state bg-state-info fa fa-ban" href="javascript:;" onclick="app.kcpd.zf('@item.id',this)" title="作废"></a>
                                                                }*@
                                                            </div>
                                                        </div>
