<script type="text/javascript">
    
    app.kcpd = app.kcpd || {};

    //分页回调
    app.kcpd.list_pagination_callback = function (data, textStatus, idx, jq) {
        app.kcpd.list_search_callback(data, textStatus);
    };

    //查询回调
    app.kcpd.list_search_callback = function (data, textStatus) {
        var sort_array = $('.table a[data-sort]', _);
        var $current_sort = ($('#hid_sort', _).val().toLowerCase()).split(' ', 2);       //这是当前排序

        //绑定UI
        sort_array.each(function (index, obj) {
            var $current = $(obj);
            if ($current.attr('data-sort') === $current_sort[0]) {
                $current.attr('data-dir', $current_sort[1]);
                if ($current_sort[1] === 'asc')
                    $current.append(app.sort_icon.asc);      //升序
                else
                    $current.append(app.sort_icon.desc);     //降序
            }
            else {
                $current.append(app.sort_icon.sort);
            }
        });

        //绑定事件
        sort_array.off('click').on('click', function (e) {
            var e = e || event;
            var $current = $(e.target);        //获取当前点击派序列
            var _dir = 'desc';
            if ($.trim($current.attr('data-dir')).length == 0 || $.trim($current.attr('data-dir')) === 'asc') {
                $current.attr('data-dir', _dir);
            }
            else {
                _dir = 'asc';
                $current.attr('data-dir', _dir);
            }
            $('#hid_sort', _).val($current.attr('data-sort') + ' ' + _dir);
            app.search.do_search_kcpd_list();
        });
    };

    //编辑
    app.kcpd.edit = function (id, obj) {
        $(obj).trigger('blur');
        $.fn.menuTab.load({ url: '/kcpd/edit?id='+id, 'title': '编辑库存盘点', id: 'kcpd/add', nocache: '0' });
    };
    app.kcpd.detial = function (id, obj) {
        $(obj).trigger('blur');
        $.fn.menuTab.load({ url: '/kcpd/detial?id=' + id, 'title': '库存盘点详情', id: 'kcpd/add', nocache: '0' });
    };
    //编辑
    app.kcpd.copy = function (id, obj) {
        $(obj).trigger('blur');
        $.fn.menuTab.load({ url: '/kcpd/copy?id=' + id, 'title': '复制库存盘点', id: 'kcpd/add', nocache: '0' });
    };
    //新增
    app.kcpd.add = function (obj) {
        $(obj).trigger('blur');
        $.fn.menuTab.load({ url: '/kcpd/add', 'title': '新增库存盘点', id: 'kcpd/add', nocache: '0' });
    };

    //删除
    app.kcpd.del = function (id, obj) {
        $(obj).trigger('blur');
        $.messager.confirm("提示", "确定删除吗?", function () {

            //$.post($.DHB.U('kcpd/delete'),
            //    {
            //        id: id
            //    },
            //    function (data) {
            //        if (data.status == "success") {
            //            $.DHB.message({ 'content': data.message, 'time': 1000, 'type': 's' });
            //            $.DHB.url('kcpd/list', 'cache');
            //        }
            //        else {
            //            $.DHB.message({ 'content': data.message, 'time': 0, 'type': 'e' });
            //        }
            //    }, 'json');
            app.httpAjax.post({
                data: { id: id },
                headers: {},
                url: $.DHB.U('kcpd/delete'),
                type: "POST",
                dataType: 'json',
                beforeSend: null,
                success: function (data) {
                    if (data.status == "success") {
                        $.DHB.message({ 'content': data.message, 'time': 1000, 'type': 's' });
                        $.DHB.url('kcpd/list', 'cache');
                    }
                    else {
                        $.DHB.message({ 'content': data.message, 'time': 0, 'type': 'e' });
                    }
                },
                error: null,
                complete: null
            });

        });
    };
    //作废
    app.kcpd.zf = function (id, obj) {
        $(obj).trigger('blur');
        $.messager.confirm("提示", "确定作废吗?", function () {
            //$.post($.DHB.U('kcpd/zf'),
            //    {
