                                                        <input type="hidden" name="start_time" id="start_time" value="">
                                                        <button type="button" class="btn btn-default btn-sm m-l-none text-left" onclick="app.search.wdatepicker_kcpd_list(this);" title="开始日期" data-title="开始日期" data-field="start_time" data-type="start" style="width: 80px;">开始日期</button>
                                                        <button type="button" class="btn btn-default disabled btn-sm"><i class="glyphicon glyphicon-calendar" style="min-width: 7%;"></i></button>
                                                        <button type="button" class="btn btn-default btn-sm text-left" onclick="app.search.wdatepicker_kcpd_list(this);" data-field="start_time" data-type="end" data-position="" style="width: 80px;" title="结束日期">结束日期</button>
                                                        <input type="hidden" name="start_time_end" id="start_time_end" value="">
                                                    </div>
                                                </div>
                                                <!-- 内容搜索 -->
                                                <div class="pull-right m-r-sm" style="margin-top: 7px">
                                                    <div class="1" style="position: relative;" onmouseenter="$(this).parent().find('span:last()').show();" onmouseleave="$(this).parent().find('span:last()').hide();">
                                                        <input type="text" value="" onkeypress="
app.search
    .trigger_search_kcpd_list(event);" class="form-control input-sm search-h inline v-middle search-result search" name="s_dh" id="s_dh" style="width: 250px; border-right: none; border-radius: 2px 0 0 2px;" placeholder="单号" ondblclick="app.search.search_item_clear_kcpd_list(this);"/><span>
                                                                <button type="button" onclick="app.search
    .do_search_kcpd_list(this);" id="btn-search" class="btn btn-sm btn-default search-btn">
                                                                    <i class="fa fa-search"></i>
                                                                </button>
                                                            </span>
                                                        <span onclick="app.search.search_item_clear_kcpd_list(this);" style="display: none; position: absolute; right: 27px; top: 4px; padding: 2px 2px 2px 4px; cursor: pointer; color: #d5d3d5; width: 19px; height: 25px; background: #fff;">
                                                                <i class="fa fa-times-circle"></i>
                                                            </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <script>
                                            app.c.public_data['kcpd/list'] = app.c.public_data['kcpd/list'] || {};
                                            app.c.public_data['kcpd/list']['search_all'] = false;
                                            var currentSearch = 'app.c.public_data[\'kcpd/list\'][\'search\']';
                                            eval(currentSearch + ' = false;');
                                            $.DHB._search = function () {
                                                eval('if(' + currentSearch + '===false){app.search.search_kcpd_list();' + currentSearch + '=true;}');
                                            }

                                            app.search = app.search || {};

                                            app.search.search_kcpd_list = function () {
                                                app.c.public_data['kcpd/list'] = app.c.public_data['kcpd/list'] || {};
                                                app.c.public_data['kcpd/list']['_row_total_'] = '@pageList.ItemCount';
                                                app.c.public_data['kcpd/list']['_page_size_'] = '@pageList.PageSize';
                                                app.c.public_data['kcpd/list']['_current_page_'] = '@(pageList.PageIndex)';
                                                app.search.do_search_pagination_kcpd_list();
                                            }


                                            app.search.trigger_search_kcpd_list = function (event) {
                                                //绑定回车键
                                                event = event || window.event
                                                if (event.keyCode == 13) {
                                                    app.search.do_search_kcpd_list();
                                                    event.preventDefault();
                                                }
                                            }

                                            //查询
                                            app.search.do_search_kcpd_list = function (el, isExport) {
                                                isExport = isExport || 0;
                                                var v = $(_ + ".filter-form").serialize();
                                                var url = '/kcpd/list?_search_=1&' + v;
                                                if (isExport == 1) {
                                                    url += '&export=1';
                                                    url = $.DHB.U(url);
                                                    window.location.href = url;
                                                }
                                                else {
                                                    var options = {
                                                        url: $.DHB.U(url),
                                                        dataType: 'html',
                                                        beforeSend: function () {
                                                            $.DHB.showButterbar();
                                                        },
                                                        success: function (data, textStatus, jqXHR) {
                                                            if ($('#kcpd-list-list-fresh-box', _).length > 0) {
                                                                $('#kcpd-list-list-fresh-box', _).html(data);
                                                            }
                                                            var curSearchcallback = 'app.kcpd.list_search_callback';
                                                            var curListcallback = 'app.kcpd.list_listready';
                                                            eval('try {if(' + curSearchcallback + ' && typeof(' + curSearchcallback + ')=="function"){' + curSearchcallback + '(data, textStatus); }}catch(e){}');
                                                            eval('try {if(' + curListcallback + ' && typeof(' + curListcallback + ')=="function"){' + curListcallback + '(); }}catch(e){}');
                                                            app.search.do_search_pagination_kcpd_list();
                                                        },
                                                        complete: function (XHR, TS) {
                                                            $.DHB.closeButterbar();
                                                        }
                                                    };
                                                    //cy.http.Get(options);
                                                    app.httpAjax.post(options);
                                                }
                                                return false;
                                            };

                                            //分页
                                            app.search.do_search_pagination_kcpd_list = function () {
                                                $.DHB.func.pagination({
                                                    module_name: 'Manager',
                                                    controller_name: 'kcpd',
                                                    action_name: 'list',
                                                    ready_once: false
                                                });
                                            }


                                            app.search.do_search_option_kcpd_list = function (el) {
                                                if ($(el).attr('class') != '') {
                                                    $(el)
                                                        .find('span')
                                                        .each(function () {
                                                            if ($(this).attr('class') == '') {
                                                                $(this).attr('class', 'order-active');
                                                            } else {
                                                                $(this).attr('class', '');
                                                            }
                                                        });

                                                    if ($(el).find('span:eq(0)').attr('class') == '') {
                                                        $(el).parents('.btn-group').find('input:eq(1)').val('desc');
                                                    } else {
                                                        $(el).parents('.btn-group').find('input:eq(1)').val('asc');
                                                    }
                                                }

                                                if ($(el).attr('class') == '') {
                                                    $(el).parent().find('.active').removeClass('active');
                                                    $(el).addClass('active');
                                                    $(el)
                                                        .parents('.btn-group')
                                                        .find('input:eq(0)')
                                                        .val($(el).attr('value'));
                                                }

                                                $(el).parents('.btn-group').removeClass('open');
                                                var selectA = $(el).find('a').clone();
                                                selectA.find('span[class=""]').remove();
                                                $(el)
                                                    .parents('.btn-group')
                                                    .find('button span:eq(0)')
                                                    .html(selectA.html());

                                                app.search.do_search_kcpd_list();
                                            }


                                            app.search.search_enter = function (el) {
                                                if (app.c.public_data['kcpd/list']['search_all'] === false) {
                                                    $(el)
                                                        .parents('.search-box-container')
                                                        .find(".bootstrap-select ul.dropdown-menu")
                                                        .each(function () {
                                                            var ts = $(this).find('li:first() span:first()');
                                                            ts.text('全部');
                                                        });
                                                    /*$(el).parents('.search-box-container').find(".treeview .list-group>li:first()").each(function(){
                                                        $(this).text('全部');
                                                    });*/
                                                    app.c.public_data['kcpd/list']['search_all'] = true;
                                                }
                                            }


                                            app.search.search_leave = function (el) {
                                            }

                                            app.search.search_client_clear_kcpd_list = function (el) {
                                                $(el).parents('.input-group').find('input').val('');
                                                app.search.do_search_kcpd_list();
                                            }


                                            app.search.wdatepicker_kcpd_list = function (el) {
                                                var booStart = $(el).data('type') == 'start';
                                                var option = {};
                                                option['el'] = $(el).data('field') + (!booStart ? '_end' : '');
                                                option['onpicked'] = function () {
                                                    $(el)
                                                        .text($dp.cal.getP('y') +
                                                            '-' +
                                                            $dp.cal.getP('M') +
                                                            '-' +
                                                            $dp.cal.getP('d'));
                                                    if (booStart) {
                                                        setTimeout(function () {
                                                                $(el)
                                                                    .parent()
                                                                    .find('[data-type="end"]')
                                                                    .data('position', '1')
                                                                    .click();
                                                            },
                                                            5000);
                                                    }
                                                    app.search.do_search_kcpd_list();
                                                };

                                                if (booStart) {
                                                    option['maxDate'] =
                                                        '#F{ $dp.$D(\'' + $(el).data('field') + '_end\') }';
                                                } else {
                                                    option['minDate'] = '#F{ $dp.$D(\'' + $(el).data('field') + '\') }';
                                                    if ($(el).data('position') == '1') {
                                                        //option['position'] ={left:$(el).offset().left,top:$(el).offset().top+34};
                                                        //$(el).data('position','');
                                                    }
                                                }
                                                option['oncleared'] = function () {
                                                    $(el).html(booStart ? $(el).data('title') : '截至日期');
                                                    app.search.do_search_kcpd_list();
                                                };

                                                WdatePicker(option);
                                            }


                                            app.search.search_item_clear_kcpd_list = function (el) {
                                                var par = $(el).parents('form');
                                                par.get(0).reset();
                                                par.find("select.select2_")
                                                    .selectpicker({ liveSearch: true })
                                                    .selectpicker('refresh');
                                                par.find("select.select2").selectpicker('refresh');
                                                app.search.do_search_kcpd_list();
                                            }
                                        </script>
                                    </div>

                                    <input id="hid_sort" type="hidden" name="sort" value="@sort" />
                                </form>
                            </div>
                        </div>
                        @if (IsPermissionShow("add", actionlist))
                        {
                            <div class="col-xs-2 pull-left">
                                <div class="btn-group m-t-12 f-l">                                
                                    <button type="button" class="btn btn-info" onclick="app.kcpd.add(this)"><span class="fa fa-plus btn_icon"></span>新增</button>
                                </div>
                            </div>
                        }
                    </div>
                    <!--数据表格-->
                    @* //TODO: 此ID名称一定要按格式要求Controller-Action-list-fresh-box *@
                    <div id="kcpd-list-list-fresh-box" class="table_list table_max_h">
                        <table class="table m-b-none row1 selectAll-table status-box one-line" onmouseover="$.DHB.func.listOperate(this);" style="table-layout: fixed">
                            <thead>
                                <tr>
