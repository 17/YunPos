            shopsp_e.sl_total = $.trim(sl_total);

            shopsp_e.je_cb = $.trim($(this).find('input[name="je_cb"]').val()); //成本金额
            shopsp_e.je_yk = $.trim($(this).find('input[name="je_yk"]').val()); //盈亏金额

            if (shopsp_e.id_shopsp != '') {
                shopsp.push(shopsp_e);
            }
            xh++;


            //当前金额
            var moneyTotal = limit_num(shopsp_e.je_yk.toString(), "@digit["dj_digit"]")
            //总金额
            //totleMoney += parseFloat($.trim(moneyTotal))
            totleMoney = accAdd(totleMoney, parseFloat($.trim(moneyTotal)));
        });

