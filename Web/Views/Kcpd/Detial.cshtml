<script type="text/javascript">
    app.kcpd = app.kcpd || {};
    app.kcpd.option = '@option';
    $.DHB._ = function () {
        app.c.public_data['kcpd/add'] = app.c.public_data['kcpd/add'] || {};
        if (app.c.public_data['kcpd/add']['once'] === false) {
            app.c.public_data['kcpd/add']['once'] = true;
            $.DHB.checkForm(function () {
                //
                if ($(_ + "#shopsp_table>tbody>tr[data-item!='']").length <= 0) {
                    $.DHB.message({ 'content': '请至少选择一个商品！', 'type': 'i' });
                }
                else if ($(_ + '#id_shop').val() === $(_ + '#id_shop_rk').val() && $(_ + '#id_shop').val() != '') {
                    $.DHB.message({ 'content': '制单门店、入库门店不能为同一门店！', 'type': 'i' });
                }
                else {
                    //
                    app.kcpd.setresult();
                    var objData = {};
                    objData.json_data = $(_ + 'input[name="table_result"]').val();
                    objData.bz = $(_ + '#remark').val();
                    objData.id_shop = $(_ + '#id_shop').val();
                    objData.id_jbr = $(_ + '#id_jbr').val();
                    objData.dh = $(_ + '#dh').val();
                    objData.rq = $(_ + '#rq').val();
                    objData.id = $(_ + '#edit_id').val();
                    objData.je_yk_mxtotal = $(_ + '#edit_goods_total').text();

                    @*$.post($.DHB.U('kcpd/@option'), objData,
                        function (data) {
                            if (data.status == "success") {
                                $.DHB.message({ 'content': data.message, 'time': 4000, 'type': 's' });
                                if ($(_ + '#add_new').val() == '0') {
                                    $.DHB.close('kcpd/add');
                                    $.DHB.url('kcpd/list', 'cache', '库存盘点');
                                }
                                else {
                                    $.fn.menuTab.deleteMenu('kcpd/list');
                                    $.fn.menuTab.load({ url: '/kcpd/add', 'title': '新增库存盘点', id: 'kcpd/add', nocache: '1' });
                                }
                            } else {
                                $.DHB.message({ 'content': data.message, 'time': 0, 'type': 'e' });
                            }
                        }, 'json'
                    );*@

                    app.httpAjax.post({
                        data: objData,
                        headers: {},
                        url: $.DHB.U('kcpd/@option'),
                        type: "POST",
                        dataType: 'json',
                        beforeSend: null,
                        success: function (data) {
                            if (data.status == "success") {
                                $.DHB.message({ 'content': data.message, 'time': 4000, 'type': 's' });
                                if ($(_ + '#add_new').val() == '0') {
                                    $.DHB.close('kcpd/add');
                                    $.DHB.url('kcpd/list', 'cache', '库存盘点');
                                }
                                else {
                                    $.fn.menuTab.deleteMenu('kcpd/list');
                                    $.fn.menuTab.load({ url: '/kcpd/add', 'title': '新增库存盘点', id: 'kcpd/add', nocache: '1' });
                                }
                            } else {
                                $.DHB.message({ 'content': data.message, 'time': 0, 'type': 'e' });
                            }
                        },
                        error: null,
                        complete: null
                    });

                }
                return false;
            });
            //
            $(function () {
                $(_ + '#submit-button').removeAttr('disabled');
            });
        }
    };



    app.kcpd.id_shop_master = '@ViewData["id_shop_master"]';





    app.kcpd.removeAll = function () {
        $('#shopsp_table>tbody', _).find('tr').each(function () {
            $(this).remove();
        });
    }

    //回填金额 callback
    app.kcpd.addshopsp_extendmoney_callback = function () {
    }

    //表头备注
    app.kcpd.orders_remark = function () {
        $(_ + "#orders-remark").modal('show');
        $(_ + "#orders-remark textarea[name='remark_value']").val($(_ + "#remark").text());
    }

    //表头备注保存
    app.kcpd.orders_remark_save = function () {
        $(_ + "#orders-remark").modal('hide');
        $(_ + "#remark").text($(_ + "#orders-remark textarea[name='remark_value']").val());
    }
    app.kcpd.dialogCallBack = function (array) {

        var jsonStr = array[5].value;
        app.kcpd.dialogCallBackWork(jsonStr);
        //关闭dialog
        $.DHB.dialog({ 'id': 'dialog-shopsp-search', 'action': 'destroy' });
        app.kcpd.setresult(true);
    }
    //选择商品回调
    app.kcpd.dialogCallBackWork = function (jsonStr) {

        var data_item_last = '';
        var objList = jQuery.parseJSON(jsonStr);
        $(objList).each(function (i, obj) {
            var guid = parseInt($(_ + "#shopspnum").val()) + 1;
            var option = '';
            var resultHtml = '';
            resultHtml += '<tr data-item="' + obj.id_shopsp + '">';
            resultHtml += '<td class="align_center">  <div name="xh">' + guid + '</div>  <input readonly="readonly" class="form-control user-input" style="width:95%;display:none;" placeholder="商品ID" name="shopsp_obj" type="text" value="' + obj.id_shopsp + '" data-id_sp="' + obj.id_sp + '" data-id_kcsp="' + obj.id_kcsp + '" ></td>';

            resultHtml += ' <td><label class="m-l-xs1" style="width:87%;"><input readonly="readonly" class="form-control user-input" style="width:98%;border:0px;"   type="text" value="' + obj.barcode + '" name="barcode" old-data="' + obj.barcode + '"></label></td>';
            resultHtml += '<td name="mc"> <div style="width: 100% !important;text-align:left;padding:0 0;"><span> ' + obj.mc + '</span></div> </td>';
            if (typeof (obj.dw) != 'undefined' && obj.dw != 'undefined' && obj.dw != '') {

                resultHtml += '<td name="dw">    <div id="dw_select" name="dw_select"  class="common_select" style="display:block;"><div><span class="inline" data-zhl="' + limit_num(obj.zhl.toString(), "@digit["sl_digit"]") + '">' + obj.dw + '</span> <i class="fa fa-caret-down"></i></div><ul class="common_select_list"><li value="' + limit_num(obj.dj_ps == 0 ? obj.dj_jh.toString() : obj.dj_ps.toString(), "@digit["dj_digit"]") + '"  data-zhl="' + limit_num(obj.zhl.toString(), "@digit["sl_digit"]") + '"    data-id_shopsp="' + obj.id_shopsp + '"   data-id_kcsp="' + obj.id_kcsp + '"  data-id_sp="' + obj.id_sp + '"  data-dw="' + obj.dw + '"  >' + obj.dw + '</li></ul></div></td>';
