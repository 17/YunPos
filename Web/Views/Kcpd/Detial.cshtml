                                                    <th class="width120">总数量</th>
                                                    <th class="width120">库存数量</th>
                                                    <th class="width120">盈亏数量</th>
                                                    <th class="width120">进货价</th>
                                                    <th class="width120">零售价</th>
                                                    <th class="width120">成本金额</th>
                                                    <th class="width120">盈亏金额</th>
                                                    <th class="width200">备注</th>
                                                    <th width="55">操作</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in item_copy.model2List)
                                                {
                                                    <tr data-item="@item.id_shopsp">
                                                        <td class="align_center"><div name="xh">1</div><input class="form-control user-input" readonly="readonly" style="width:95%;display:none;" placeholder="商品ID" name="shopsp_obj" type="text" value="@item.id_shopsp" data-id_kcsp="@item.id_kcsp" data-id_sp="@item.id_sp"></td>
                                                        <td>
                                                            <label class="m-l-xs1" style="width:87%;">
                                                                <input readonly="readonly" class="form-control user-input" style="width:98%;border:0px;"    type="text" value="@item.barcode" name="barcode" old-data="@item.barcode">
                                                            </label>
                                                        </td>
                                                        <td name="mc">
                                                            <div style="width: 100% !important;text-align:left;padding:0 0;"><span>@item.shopsp_name</span></div>
                                                        </td>
                                                        <td name="dw" class="align_center">
                                                            @item.dw 
                                                        </td>
                                                        <td>
                                                            <input readonly="readonly" class="form-control user-input" style="width: 96%; border: 0px; text-align: right;" placeholder="数量" type="text" value="@item.sl.Digit((int) digit["sl_digit"])" name="sl" onkeyup="check_digit(this, '@digit["sl_digit"]')" onafterpaste="check_digit(this,'@digit["sl_digit"]')" old-data="@item.sl.Digit((int) digit["sl_digit"])">
                                                            <input name="sl_zhl" style="width: 95%; display: none; border: 0px; text-align: right;" placeholder="转换率" type="text" value="@item.zhl">
                                                        </td>
                                                        <td>
                                                            <input readonly="readonly" class="form-control user-input" name="sl_total" style="width: 95%; border: 0px; text-align: right;" placeholder="总数量" type="text" value="@item.sl_total.Digit((int)digit["sl_digit"])">
                                                        </td>
                                                        <td>
                                                            <input readonly="readonly" class="form-control user-input"  name="sl_kc" style="width: 95%; border: 0px; text-align: right;" placeholder="库存数量" type="text" value="@item.sl_kc.Digit((int)digit["sl_digit"])">
                                                        </td>
                                                        <td>
                                                            <input readonly="readonly" class="form-control user-input" name="sl_yk" style="width: 95%; border: 0px; text-align: right;" placeholder="盈亏数量" type="text" value="@item.sl_yk.Digit((int)digit["sl_digit"])">
                                                        </td>
                                                        <td>
                                                            <input readonly="readonly" class="form-control user-input" style="width: 96%; border: 0px; text-align: right;" placeholder="进货价(元)" type="text" value="@item.dj_jh.Digit((int) digit["dj_digit"])" name="dj_jh" onkeyup="check_digit(this, '@digit["dj_digit"]')" onafterpaste="check_digit(this,'@digit["dj_digit"]')" old-data="@item.dj_jh.Digit((int) digit["dj_digit"])">
                                                        </td>
                                                        <td>
                                                            <input readonly="readonly" class="form-control user-input" style="width:96%;border:0px;text-align:right;" placeholder="零售价(元)" type="text" value="@item.dj_ls.Digit((int)digit["dj_digit"])" name="dj_ls" onkeyup="check_digit(this,'@digit["dj_digit"]')" onafterpaste="check_digit(this,'@digit["dj_digit"]')" old-data="@item.dj_ls.Digit((int)digit["dj_digit"])">
                                                        </td>
                                                        <td name="je_cb">
                                                            <input readonly="readonly" class="form-control user-input" style="width: 96%; border: 0px; text-align: right;" placeholder="成本金额(元)" type="text" value="@item.je_cb.Digit((int) digit["je_digit"])" name="je_cb" onkeyup="check_digit(this, '@digit["je_digit"]')" onafterpaste="check_digit(this,'@digit["je_digit"]')" old-data="@item.je_cb.Digit((int) digit["je_digit"])">
                                                        </td>
                                                        <td name="je_yk">
                                                            <input readonly="readonly" class="form-control user-input" style="width:96%;border:0px;text-align:right;" placeholder="盈亏金额(元)" type="text" value="@item.je_yk.Digit((int)digit["je_digit"])" name="je_yk" onkeyup="check_digit(this,'@digit["je_digit"]')" onafterpaste="check_digit(this,'@digit["je_digit"]')" old-data="@item.je_yk.Digit((int)digit["je_digit"])">
                                                        </td>
                                                        <td><input readonly="readonly" class="form-control user-input" style="width:96%;border:0px;"   type="text" value="@item.bz" name="bz"></td>
                                                        <td></td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>

                                        <div class="cache-init-goods"></div>
                                        <input type="hidden" name="table_result" id="table_result" value="" />
                                    </div>
                                    <div class="padding1 border_all pad-b5">
                                        <button class="btn btn-info w-xs" type="button" disabled onclick="app.kcpd.addshopsp_row(this)">增加行</button>
                                        <div class="f-r notice-p">
                                            <div class="w-295 f-l ">
                                                <span class="fo_l">合计金额：￥</span>
                                                <span id="edit_goods_total" class="fo_r">0.00</span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                            <div style="clear:both;"></div>
                            <div class="jh_add_down h-30">
                                <div class="f-r mr20"><span class="fo_l">审核时间：</span><span class="fo_r">@(item_copy.model1.rq_sh == null ? "" : ((DateTime)item_copy.model1.rq_sh) < new DateTime(2000, 01, 01) ? "" : ((DateTime)item_copy.model1.rq_sh).ToString("yyyy-MM-dd HH:mm:ss"))</span></div>
                                <div class="f-r mr20"><span class="fo_l">审核人：</span><span class="fo_r">@(ViewData["shr_name"] == null ? "" : ViewData["shr_name"].ToString())</span></div>
                                <div class="f-r mr20"><span>制单时间：</span><span class="fo_r">@(item_copy.model1.rq_create == null ? DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss") : ((DateTime)item_copy.model1.rq_create).ToString("yyyy-MM-dd HH:mm:ss"))</span></div>
                                <div class="f-r mr20"><span class="fo_l">制单人：</span><span class="fo_r">@(ViewData["zdr_name"] == null ? "" : ViewData["zdr_name"].ToString())</span></div>
                            </div>
                        </div>
                        
                        
                    </div>
                </div>
                <footer class="panel-footer  lter need-footer-fixed align_left">

                    <input type="hidden" name="add_new" id="add_new" value="" /> 
                    <button type="button" class="btn w-xs btn-default" title="调拨出库" onclick="$.fn.menuTab.load({ url: '/kcpd/list', 'title': '调拨出库', id: 'kcpd/list', nocache: '0' }); $.DHB.close('kcpd/add')">取消</button>
                </footer>
            </form>
        </div>
    </div>
</div>



